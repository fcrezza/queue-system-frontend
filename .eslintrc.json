{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["react-app", "airbnb", "prettier", "prettier/react"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["prettier", "react"],
	"rules": {
		"no-unused-vars": 1,
		"react/prop-types": 0,
		"react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
		"react/jsx-props-no-spreading": 0,
		"jsx-a11y/label-has-associated-control": 0
	}
}
